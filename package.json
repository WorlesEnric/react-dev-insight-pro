{
  "name": "react-dev-insight-pro",
  "version": "1.0.0",
  "description": "AI-powered React component inspector with intelligent code refactoring",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev --workspace=@react-dev-insight/server",
    "dev:client": "npm run dev --workspace=@react-dev-insight/client",
    "build": "npm run build --workspaces",
    "build:server": "npm run build --workspace=@react-dev-insight/server",
    "build:client": "npm run build --workspace=@react-dev-insight/client",
    "build:injector": "npm run build --workspace=@react-dev-insight/injector",
    "start": "npm run start --workspace=@react-dev-insight/server",
    "lint": "eslint packages/*/src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf packages/*/dist packages/*/node_modules node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/react-dev-insight-pro.git"
  },
  "keywords": [
    "react",
    "devtools",
    "ai",
    "refactoring",
    "code-optimization",
    "developer-tools"
  ],
  "author": "React Dev Insight Team",
  "license": "MIT"
}
